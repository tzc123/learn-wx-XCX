<template name="nav">
  <view class="top-bar">
    <block wx:for="{{navList}}" wx:key="">
      <view id="{{item.id}}" data-index="{{index}}"
      catchtap="onTapTag"
      class="top-bar-item {{index==activeIndex?'top-bar-active':''}}">
        <text>{{item.title}}</text>
      </view>
    </block>
  </view>
</template>
